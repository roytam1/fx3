<page xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
	onload="parent.initPanel('chrome://calendar/content/pref/timezonePrefs.xul');"
      headertitle="&calendar.timezone.label;">

   <script type="application/x-javascript" src="chrome://global/content/strres.js"/>
   <script type="application/x-javascript" src="chrome://calendar/content/calendarUtils.js"/>
   
  <script type="application/x-javascript">
    <![CDATA[
       
      /* 
      ** NOTE: 
      ** When you add a pref to this page, you have to add its ID to the following list.
      */
       
      var _elementIDs = [
      "defaultzone"
      ];

      var gStartupValue = false;
      

      function Startup() {
          // If nothing was choosen, set default timezone to the guessed timezone
          var elem = document.getElementById("defaultzone");
          if (!elem.value) {
              elem.value = calendarDefaultTimezone();
              // Set the prefstring to something else, so that the selected
              // value won't get saved in the real pref
              elem.setAttribute("prefstring", "calendar.timezone.temp");
              // But on changes, set the real prefstring. We do want to save now!
              elem.addEventListener(
                  "ValueChange", 
                  function() { elem.setAttribute("prefstring", "calendar.timezone.local"); }, 
                  false);
          }
      }
    ]]>
  </script>

