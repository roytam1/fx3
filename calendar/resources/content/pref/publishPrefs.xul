<?xml-stylesheet href="chrome://communicator/skin/" type="text/css"?>

<!-- DTD File with all strings specific to the file -->
<!DOCTYPE page 
[
    <!ENTITY % dtd1 SYSTEM "chrome://calendar/locale/prefs.dtd" > %dtd1;
]>

<page xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
      onload="parent.initPanel('chrome://calendar/content/pref/publishPrefs.xul');"
      headertitle="&calendar.publish.label;">

   <script type="application/x-javascript" src="chrome://calendar/content/dateUtils.js"/>
   <script type="application/x-javascript" src="chrome://global/content/strres.js"/>
   
   <script type="application/x-javascript">
      var _elementIDs = [
         "publishPath"
      ];
   </script>

   <groupbox>
      <caption label="&pref.publish.caption;"/>
      <hbox>
         <description>&pref.publish.path.label;</description>
         <textbox flex="1" id="publishPath" prefstring="calendar.publish.path"/>
      </hbox>
   </groupbox>
</page>
