#
# .twmrc, using this for tinderbox.
#

RandomPlacement

TitleFont "-adobe-helvetica-bold-r-normal--*-100-*-*-*-*-*-*"

Button1 = : root : f.menu "b1"


menu "b1"
{
"xterm"  f.exec "xterm &"
"Quit"             f.menu "quit"
}

menu "quit"
{
"Really Quit?"     f.title
"No"               f.nop
"Yes"              f.quit
}
